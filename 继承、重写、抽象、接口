class father
{
    private String name;
    private int age;

    public father(){}
    public father(String name, int age)
    {
        this.name = name;
        this.age = age;
    }
    public void prin()
    {
        System.out.println("father");
    }
    public void setName(String name) {
        this.name = name;
    }

    public void setAge(int age) {
        this.age = age;
    }

    public String getName() {
        return name;
    }
    public int getAge() {
        return age;
    }
}
