class outter
{
    private int age;
    private String name = "outter";

    public void show()
    {
        //print();å¤–éƒ¨ç±»ä¸èƒ½ç›´æ¥è®¿é—®å†…éƒ¨ç±»çš„å±æ€§
        new InnerClass().print();//æ–°å»ºå†…éƒ¨ç±»å®ä¾‹è°ƒç”¨å†…éƒ¨ç±»æ–¹æ³•
        System.out.println(new InnerClass().id);//é€šè¿‡æ–°å»ºå®ä¾‹å¯ä»¥è®¿é—®å†…éƒ¨ç±»çš„ç§æœ‰å±æ€§
    }
    class InnerClass
    {
        private int id = 1;
        private String name = "inner";

        public void print() {
            age=2;//å†…éƒ¨ç±»å¯ä»¥ç›´æ¥è®¿é—®å¤–éƒ¨ç±»çš„ç§æœ‰å±æ€§
            System.out.println(name);//å†…éƒ¨ç±»ä¸­é»˜è®¤è®¿é—®å†…éƒ¨ç±»é‡åå±æ€§
            System.out.println(this.name);
            System.out.println(outter.this.name);//å¯ä»¥ä½¿ç”¨outter.thisè®¿é—®å¤–éƒ¨é‡åå±æ€§
        }
//        public static void fail(){}å†…éƒ¨ç±»ä¸èƒ½å®šä¹‰staticæ–¹æ³•
    }
}
